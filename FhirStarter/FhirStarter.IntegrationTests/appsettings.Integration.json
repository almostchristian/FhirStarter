{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "FhirEngine": {
    "ShowExceptionDetails": true,
    "SystemPlugins": {
      "HealthCheck": {
        "Detailed": true
      }
    },
    "Handlers": {
      "RepositoryHandlers": {
        "DbRepository": {
          "FhirDataStore<@NoSql>": {
            "UsePlatformSpecificSqlConnectionString": {
              "DatabaseName": "FhirStarter_Document"
            },
            "UseSqlDocument": {
              "Migration": {
                "DeployDatabase": true,
                "CreateNewDatabase": true
              }
            }
          }
        }
      }
    }
  }
}
